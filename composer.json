{
    "name": "russsiq/laravel-assistant",
    "description": "Ассистент приложения на Laravel 6.x.",
    "homepage": "https://github.com/russsiq/laravel-assistant",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "laravel-assistant", "laravel-app-manager", "laravel-installer", "laravel-updater"],
    "authors": [{
        "name": "Рустам Гимранов",
        "email": "rustam_gimranov@mail.ru",
        "homepage": "https://github.com/russsiq",
        "role": "Developer"
    }],
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/russsiq/laravel-assistant"
    }],
    "require": {
        "illuminate/support": "6.*",
        "php": ">=7.2.0",
        "russsiq/laravel-env-manager": "dev-master"
    },
    "require-dev": {},
    "autoload": {
        "files": [
            "src/app/Support/helpers.php"
        ],
        "psr-4": {
            "Russsiq\\Assistant\\": "src/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Russsiq\\Assistant\\AssistantServiceProvider"
            ],
            "aliases": {
                "Assistant": "Russsiq\\Assistant\\Support\\Facades\\Assistant"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
